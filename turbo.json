{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build", "^db:generate"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "dependsOn": ["^db:generate"],
      "inputs": ["$TURBO_DEFAULT$"]
    },
    "db:push": {
      "dependsOn": ["^db:push"],
      "inputs": ["$TURBO_DEFAULT$"]
    }
  }
}
